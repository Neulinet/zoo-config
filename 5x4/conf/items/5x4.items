Group gZooPower
Group gZooPowerChart
Group gZooPowerDay
Group gZooPowerWeek
Group gZooPowerMonth

Group gRooms

DateTime billPeriod_start
DateTime billPeriod_end


// Rooms and Master Switches

Group:Switch:OR(ON, OFF) gGarage 	"Garage" 	(gRooms) [ "home-group", "room", "master-switch" ]
Group:Switch:OR(ON, OFF) gBedroom 	"Bedroom" 	(gRooms) [ "home-group", "room", "master-switch" ]
Group:Switch:OR(ON, OFF) gOffice 	"Office" 	(gRooms) [ "home-group", "room", "master-switch" ]
Group:Switch:OR(ON, OFF) gKitchen 	"Kitchen" 	(gRooms) [ "home-group", "room", "master-switch" ]
Group:Switch:OR(ON, OFF) gRooftop 	"Rooftop" 	(gRooms) [ "home-group", "room", "master-switch" ]

Group:Switch:OR(ON, OFF) gExterior 	"Exterior" 	(gRooms) [ "home-group", "room", "master-switch" ]
Group:Switch:OR(ON, OFF) gLaundry 	"Laundry" 	(gRooms) [ "home-group", "room", "master-switch" ]
Group:Switch:OR(ON, OFF) gEntry 	"Entry" 	(gRooms) [ "home-group", "room", "master-switch" ]
Group:Switch:OR(ON, OFF) gOther		"Other" 	(gRooms) [ "home-group", "room", "master-switch" ]


// Lights and Power per room 

// Garage
Dimmer lights_Garage_Strip_Lights "Garage Strip Lights	(gGarage) 	 [ "light" ] { channel="mox:1_gang_dimmer:Garage_Strip_Lights:state" }
Dimmer light_Garage_Shower_Lights	(gGarage) 	 [ "light" ] { channel="mox:1_gang_dimmer:Garage_Shower_Lights:state" }
Switch power_FCU "FCU"	(gGarage) 	[ "power" ] { channel="mox:1_gang_onoff:FCU:state" }
Switch power_Garage "Garage"	(gGarage) 	[ "power" ] { channel="mox:1_gang_onoff:Garage:state" }


// Bedroom

Switch power_Bedroom "Bedroom"	(gBedroom) 	[ "power" ] { channel="mox:1_gang_onoff:Bedroom:state" }
Switch power_Library "Library"	(gBedroom) 	[ "power" ] { channel="mox:1_gang_onoff:Library:state" }
Switch power_Vanity "Vanity"	(gBedroom) 	[ "power" ] { channel="mox:1_gang_onoff:Vanity:state" }
Switch power_Bathroom "Bathroom"	(gBedroom) 	[ "power" ] { channel="mox:1_gang_onoff:Bathroom:state" }

// Office

Switch power_Cupboard "Cupboard"	(gOffice) 	[ "power" ] { channel="mox:1_gang_onoff:Cupboard:state" }
Switch power_Office "Office"	(gOffice) 	[ "power" ] { channel="mox:1_gang_onoff:Office:state" }
Dimmer light_Office_Cupboard "Cupboard"	(gOffice) 	 [ "light" ] { channel="mox:1_gang_dimmer:Office_Cupboard:state" }


// Kitchen

