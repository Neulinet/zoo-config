// Rooftop
Switch power_Roof_BBQ "BBQ GPO"	(gRooftop) 	[ "power" ] { channel="mox:1_gang_onoff:Roof_BBQ:state" }
Switch power_Roof_Top "Roof top GPO"	(gRooftop) 	[ "power" ] { channel="mox:1_gang_onoff:Roof_Top:state" }
Switch power_Spa "Spa Power"	(gRooftop) 	[ "power" ] { channel="mox:1_gang_onoff:Spa:state" }
Switch power_Cooler "Cooler"	(gRooftop) 	[ "power" ] { channel="mox:1_gang_onoff:Coolers:state" }
Dimmer light_Stairs_Roof "Roof Stairs" (gRooftop) 	[ "light" ] { channel="mox:1_gang_dimmer:Stairs_Roof:state" }


//Exterior

Dimmer light_Exterior_Lights	(gExterior_Lights) 	 [ "light" ] { channel="mox:1_gang_dimmer:Exterior_Lights:state" }


//Laundry

