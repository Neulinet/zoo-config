Switch power_Glass_Wall "Glass Wall"	(gKitchen) 	[ "power" ] { channel="mox:1_gang_onoff:Glass_Wall:state" }
Switch power_Glass_Wall_2 "Glass Wall 2"	(gKitchen) 	[ "power" ] { channel="mox:1_gang_onoff:Glass_Wall_2:state" }
Switch power_Range_Hood "Range Hood"	(gKitchen) 	[ "power" ] { channel="mox:1_gang_onoff:Ranger_Hood:state" }
Switch power_Side_Bench "Side Bench"	(gKitchen) 	[ "power" ] { channel="mox:1_gang_onoff:Side_Bench:state" }
Switch power_Tv_Area_2 "Tv Area 2"	(gKitchen) 	[ "power" ] { channel="mox:1_gang_onoff:Tv_Area_2:state" }
Switch power_Kitchen_Bench "Kitchen Bench"	(gKitchen) 	[ "power" ] { channel="mox:1_gang_onoff:Kitchen_Bench:state" }
Switch power_Under_Sink "Under Sink"	(gKitchen) 	[ "power" ] { channel="mox:1_gang_onoff:Under_Sink:state" }
Switch power_Dishwasher "Dishwasher"	(gKitchen) 	[ "power" ] { channel="mox:1_gang_onoff:Dishwasher:state" }
Switch power_Cooler "Cooler"	(gKitchen) 	[ "power" ] { channel="mox:1_gang_onoff:Coolers:state" }
Dimmer light_Kitchen_track_Lights "Track Lights"	(gKitchen) 	 [ "light" ] { channel="mox:1_gang_dimmer:Kitchen_track_Lights:state" }
Dimmer light_Office_Kitchen_Strip_Lights "Stip Lights"	(gKitchen) 	 [ "light" ] { channel="mox:1_gang_dimmer:Kitchen_Strip_Lights:state" }
Dimmer light_Kitchen_Garden_Lights	"Garden Lights" (gKitchen) 	 [ "light" ] { channel="mox:1_gang_dimmer:Kitchen_Garden_Lights:state" }
